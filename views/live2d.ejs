<% if (global.config.live2d.enable_live2d) {%>
    <div id="landlord">
        <div class="message" style="opacity:0"></div>
        <canvas id="live2d" width="400" height="450" class="live2d"></canvas>
        <div class="hide-button">隐藏</div>
    </div>
    <script type="text/javascript">
        var message_Path = '/live2d/'
        var home_Path = '<%= global.config.url %>' 
    </script>
    <script type="text/javascript" src="live2d/js/live2d.js"></script>
    <!-- <script type="text/javascript" src="live2d/js/message.js"></script> -->
    <script type="text/javascript">
        loadlive2d("live2d", "live2d/model/<%= global.config.live2d.model%>/<%= global.config.live2d.conf %>");
    </script>
<% } %>
